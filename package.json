{
  "name": "nest-scraping-app",
  "version": "1.0.0",
  "scripts": {
    "start": "node -r tsconfig-paths/register dist/main.js",
    "start:dev": "nest start --watch",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\"",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:run": "typeorm-ts-node-commonjs migration:run -d ./data-source.ts",
    "migration:generate": "typeorm-ts-node-commonjs migration:generate -d ./data-source.ts -n",
    "migration:create": "typeorm-ts-node-commonjs migration:create -d ./data-source.ts -n",
    "test:e2e": "jest --config jest.e2e.config.ts --runInBand",
    "test": "jest --runInBand"
  },
  "dependencies": {
    "@nestjs/axios": "^4.0.1",
    "@nestjs/common": "^10.4.20",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^10.4.20",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.4.20",
    "@nestjs/typeorm": "^10.0.2",
    "bcryptjs": "^3.0.2",
    "body-parser": "^1.20.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "nock": "^14.0.6",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.16.3",
    "reflect-metadata": "^0.1.14",
    "rxjs": "^7.8.2"
  },
  "devDependencies": {
    "@jest/types": "^30.2.0",
    "@nestjs/cli": "^10.4.9",
    "@nestjs/mapped-types": "^2.1.0",
    "@nestjs/schematics": "^10.2.3",
    "@nestjs/testing": "^10.4.20",
    "@types/jest": "^30.0.0",
    "@types/nock": "^11.1.0",
    "@types/node": "^20.19.24",
    "@types/supertest": "^6.0.3",
    "jest": "^30.2.0",
    "prettier": "^2.8.8",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "typeorm": "^0.3.27",
    "typescript": "^5.9.3"
  }
}